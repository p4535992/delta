            <action>dialog:manageInheritablePrivilegesDialog</action>
            <action-listener>#{ManageInheritablePrivilegesDialog.init}</action-listener>
            <params>
               <param name="manageableRef">#{DocumentDialogHelperBean.nodeRef}</param>
            </params>
         </action>

