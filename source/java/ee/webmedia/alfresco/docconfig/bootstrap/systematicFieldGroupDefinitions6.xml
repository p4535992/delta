               <view:value>leaveNewEndDate</view:value>
               <view:value>leaveChangedDays</view:value>
               <view:value>leaveNewWorkYear</view:value>
            </view:values>
         </docadmin:fieldDefinitionIds>
      </view:properties>
   </docadmin:fieldGroup>

   <docadmin:fieldGroup view:childName="docadmin:leaveCancel">
      <view:properties>
         <docadmin:name>Puhkuse tühistamine</docadmin:name>
         <docadmin:systematic>true</docadmin:systematic>
