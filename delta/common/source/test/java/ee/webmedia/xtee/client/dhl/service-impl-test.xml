<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

   <!-- import XTee beans from real context -->
   <import resource="classpath:ee/webmedia/alfresco/dvk/service/dhl-service-impl-context.xml" />

   <!-- Needed only for testing (without alfresco repository) -->
   <bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
      <property name="ignoreUnresolvablePlaceholders" value="true" />
      <property name="location" value="classpath:alfresco-global.properties" />
   </bean>

</beans>
