<!--
   Praeguse andmemudeli järgi on fieldDefinition tüübil järgmised väljad:

   docadmin:fieldId - qname - mandatory, fieldDefinition view:childName peab olema sama väärtusega
   docadmin:name - text - mandatory
   docadmin:fieldType - text - mandatory
   docadmin:comment - text
   docadmin:mandatory - boolean - default puudub, järelikult false
   docadmin:mandatoryChangeable - boolean - default true
   docadmin:mandatoryForDoc - boolean - default puudub, järelikult false
   docadmin:mandatoryForVol - boolean - default puudub, järelikult false
   docadmin:changeableIf - text
   docadmin:changeableIfChangeable - boolean - default true
   docadmin:defaultValue - text
   docadmin:classificator - text
   docadmin:classificatorDefaultValue - text
   docadmin:defaultDateSysdate - boolean - default puudub, järelikult false
   docadmin:defaultUserLoggedIn - boolean - default puudub, järelikult false
   docadmin:defaultSelected - boolean - default puudub, järelikult false
   docadmin:onlyInGroup - boolean - default puudub, järelikult false
   docadmin:systematic - boolean - default false - siin peab olema alati true!
   docadmin:systematicComment - text
   docadmin:removableFromSystematicDocType - boolean - mandatory - default true - kui mandatoryForDoc=true, siis siin peab olema alati false!
   docadmin:removableFromSystematicFieldGroup - boolean - default true - omab mõtet ainult siis kui field on fieldGroup/fieldGroupDefinition all
   docadmin:isParameterInDocSearch - boolean - default puudub, järelikult false
   docadmin:isParameterInVolSearch - boolean - default puudub, järelikult false
   docadmin:isFixedParameterInDocSearch - boolean - default puudub, järelikult false
   docadmin:isFixedParameterInVolSearch - boolean - default puudub, järelikult false
   docadmin:parameterOrderInDocSearch - int
   docadmin:parameterOrderInVolSearch - int
   docadmin:docTypes - text, multiple
   docadmin:volTypes - text, multiple
   docadmin:order - int - kasutusel uue dokumendi tüübi loomisel field ja fieldGroup järjestuse määramiseks metaandmete plokis
-->

