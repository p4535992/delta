<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

   <bean id="documentConfigService" class="ee.webmedia.alfresco.docconfig.service.DocumentConfigServiceImpl">
      <property name="documentAdminService" ref="DocumentAdminService" />
      <property name="dictionaryService" ref="DictionaryService" />
      <property name="namespaceService" ref="NamespaceService" />
      <property name="userContactMappingService" ref="UserContactMappingService" />
      <property name="nodeService" ref="NodeService" />
      <property name="generalService" ref="GeneralService" />
   </bean>

   <bean id="DocumentConfigService" class="org.springframework.aop.framework.ProxyFactoryBean">
      <property name="proxyInterfaces">
         <list>
            <value>ee.webmedia.alfresco.docconfig.service.DocumentConfigService</value>
         </list>
      </property>
      <property name="target" ref="documentConfigService" />
      <property name="interceptorNames">
         <list>
            <idref bean="commonTransactionInterceptor" />
         </list>
      </property>
   </bean>

   <bean id="userContactMappingService" class="ee.webmedia.alfresco.docconfig.service.UserContactMappingServiceImpl">
      <property name="dictionaryService" ref="DictionaryService" />
      <property name="namespaceService" ref="NamespaceService" />
      <property name="nodeService" ref="NodeService" />
      <property name="generalService" ref="GeneralService" />
   </bean>

   <bean id="UserContactMappingService" class="org.springframework.aop.framework.ProxyFactoryBean">
      <property name="proxyInterfaces">
         <list>
            <value>ee.webmedia.alfresco.docconfig.service.UserContactMappingService</value>
         </list>
      </property>
      <property name="target" ref="userContactMappingService" />
      <property name="interceptorNames">
         <list>
            <idref bean="commonTransactionInterceptor" />
         </list>
      </property>
   </bean>

   <bean id="systematicClearBootstrap" class="ee.webmedia.alfresco.docconfig.bootstrap.SystematicClearBootstrap" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
      <property name="dependsOn">
         <list>
            <ref bean="docTypeSpacesBootstrap" />
            <ref bean="fieldDefinitionsBootstrap" />
         </list>
      </property>
      <property name="generalService" ref="GeneralService" />
   </bean>

   <bean id="systematicFieldDefinitionsBootstrap1" class="ee.webmedia.alfresco.common.bootstrap.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="dependsOn">
         <list>
            <ref bean="systematicClearBootstrap" />
         </list>
      </property>
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="encoding">UTF-8</prop>
               <prop key="path">/docadmin:fieldDefinitions</prop>
               <prop key="location">ee/webmedia/alfresco/docconfig/bootstrap/systematicFieldDefinitions1.xml</prop>
            </props>
         </list>
      </property>
   </bean>

   <bean id="systematicFieldDefinitionsBootstrap2" class="ee.webmedia.alfresco.common.bootstrap.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="dependsOn">
         <list>
            <ref bean="systematicFieldDefinitionsBootstrap1" />
         </list>
      </property>
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="encoding">UTF-8</prop>
               <prop key="path">/docadmin:fieldDefinitions</prop>
               <prop key="location">ee/webmedia/alfresco/docconfig/bootstrap/systematicFieldDefinitions2.xml</prop>
            </props>
         </list>
      </property>
   </bean>

   <bean id="systematicFieldDefinitionsBootstrap3" class="ee.webmedia.alfresco.common.bootstrap.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="dependsOn">
         <list>
            <ref bean="systematicFieldDefinitionsBootstrap2" />
         </list>
      </property>
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="encoding">UTF-8</prop>
               <prop key="path">/docadmin:fieldDefinitions</prop>
               <prop key="location">ee/webmedia/alfresco/docconfig/bootstrap/systematicFieldDefinitions3.xml</prop>
            </props>
         </list>
      </property>
   </bean>

   <bean id="systematicFieldDefinitionsBootstrap4" class="ee.webmedia.alfresco.common.bootstrap.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="dependsOn">
         <list>
            <ref bean="systematicFieldDefinitionsBootstrap3" />
         </list>
      </property>
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="encoding">UTF-8</prop>
               <prop key="path">/docadmin:fieldDefinitions</prop>
               <prop key="location">ee/webmedia/alfresco/docconfig/bootstrap/systematicFieldDefinitions4.xml</prop>
            </props>
         </list>
      </property>
   </bean>

   <bean id="systematicFieldGroupDefinitionsBootstrap1" class="ee.webmedia.alfresco.common.bootstrap.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="dependsOn">
         <list>
            <ref bean="systematicClearBootstrap" />
         </list>
      </property>
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="encoding">UTF-8</prop>
               <prop key="path">/docadmin:fieldGroupDefinitions</prop>
               <prop key="location">ee/webmedia/alfresco/docconfig/bootstrap/systematicFieldGroupDefinitions1.xml</prop>
            </props>
         </list>
      </property>
   </bean>

   <bean id="systematicFieldGroupDefinitionsBootstrap2" class="ee.webmedia.alfresco.common.bootstrap.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="dependsOn">
         <list>
            <ref bean="systematicFieldGroupDefinitionsBootstrap1" />
         </list>
      </property>
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="encoding">UTF-8</prop>
               <prop key="path">/docadmin:fieldGroupDefinitions</prop>
               <prop key="location">ee/webmedia/alfresco/docconfig/bootstrap/systematicFieldGroupDefinitions2.xml</prop>
            </props>
         </list>
      </property>
   </bean>

   <bean id="systematicFieldGroupDefinitionsBootstrap3" class="ee.webmedia.alfresco.common.bootstrap.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="dependsOn">
         <list>
            <ref bean="systematicFieldGroupDefinitionsBootstrap2" />
         </list>
      </property>
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="encoding">UTF-8</prop>
               <prop key="path">/docadmin:fieldGroupDefinitions</prop>
               <prop key="location">ee/webmedia/alfresco/docconfig/bootstrap/systematicFieldGroupDefinitions3.xml</prop>
            </props>
         </list>
      </property>
   </bean>

   <bean id="systematicDocumentTypesFixBootstrap" class="ee.webmedia.alfresco.docconfig.bootstrap.SystematicDocumentTypesFixBootstrap" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
      <property name="dependsOn">
         <list>
            <ref bean="systematicClearBootstrap" />
            <ref bean="systematicFieldDefinitionsBootstrap4" />
            <ref bean="systematicFieldGroupDefinitionsBootstrap3" />
         </list>
      </property>
      <property name="documentAdminService" ref="DocumentAdminService" />
   </bean>

   <bean id="systematicDocumentTypesBootstrap" class="ee.webmedia.alfresco.docconfig.bootstrap.SystematicDocumentTypesBootstrap" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="false" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="dependsOn">
         <list>
            <ref bean="systematicClearBootstrap" />
            <ref bean="systematicDocumentTypesFixBootstrap" />
            <ref bean="systematicFieldDefinitionsBootstrap1" />
            <ref bean="systematicFieldDefinitionsBootstrap2" />
            <ref bean="systematicFieldDefinitionsBootstrap3" />
            <ref bean="systematicFieldDefinitionsBootstrap4" />
            <ref bean="systematicFieldGroupDefinitionsBootstrap1" />
            <ref bean="systematicFieldGroupDefinitionsBootstrap2" />
            <ref bean="systematicFieldGroupDefinitionsBootstrap3" />
         </list>
      </property>
      <property name="generalService" ref="GeneralService" />
      <property name="documentAdminService" ref="DocumentAdminService" />
   </bean>

   <!-- Fix for task 180940 in already existing deployments -->
   <bean id="systematicMissingBootstrap" class="ee.webmedia.alfresco.docconfig.bootstrap.SystematicMissingBootstrap" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
      <property name="dependsOn">
         <list>
            <ref bean="systematicDocumentTypesBootstrap" />
         </list>
      </property>
      <property name="generalService" ref="GeneralService" />
   </bean>

   <bean id="senderEmailRemovableFromSystematicGroupFixBootstrap" class="ee.webmedia.alfresco.docconfig.bootstrap.SenderEmailRemovableFromSystematicGroupFixBootstrap" parent="abstractNodeUpdater">
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
      <property name="dependsOn">
         <list>
            <ref bean="systematicDocumentTypesBootstrap" />
         </list>
      </property>
   </bean>

   <bean id="documentLocationGenerator" class="ee.webmedia.alfresco.docconfig.generator.systematic.DocumentLocationGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
      <property name="documentService" ref="DocumentService" />
      <property name="nodeService" ref="NodeService" />
      <property name="functionsService" ref="FunctionsService" />
      <property name="seriesService" ref="SeriesService" />
      <property name="volumeService" ref="VolumeService" />
      <property name="caseService" ref="CaseService" />
      <property name="documentLogService" ref="DocumentLogService" />
   </bean>

   <!-- fieldtype generators -->

   <bean id="comboboxEditableGenerator" class="ee.webmedia.alfresco.docconfig.generator.fieldtype.ComboboxEditableGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
      <property name="classificatorService" ref="ClassificatorService" />
   </bean>

   <bean id="comboboxGenerator" class="ee.webmedia.alfresco.docconfig.generator.fieldtype.ComboboxGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
   </bean>

   <bean id="comboboxAndTextGenerator" class="ee.webmedia.alfresco.docconfig.generator.fieldtype.ComboboxAndTextGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
   </bean>

   <bean id="textFieldGenerator" class="ee.webmedia.alfresco.docconfig.generator.fieldtype.TextFieldGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
   </bean>

   <bean id="intGenerator" class="ee.webmedia.alfresco.docconfig.generator.fieldtype.IntGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
   </bean>

   <bean id="doubleGenerator" class="ee.webmedia.alfresco.docconfig.generator.fieldtype.DoubleGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
   </bean>

   <bean id="dateGenerator" class="ee.webmedia.alfresco.docconfig.generator.fieldtype.DateGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
   </bean>

   <bean id="usersContactsGenerator" class="ee.webmedia.alfresco.docconfig.generator.fieldtype.UserContactGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
   </bean>

   <bean id="checkboxGenerator" class="ee.webmedia.alfresco.docconfig.generator.fieldtype.CheckboxGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
   </bean>

   <bean id="informationTextGenerator" class="ee.webmedia.alfresco.docconfig.generator.fieldtype.InformationTextGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
   </bean>

   <!-- systematic generators -->

   <bean id="accessRestrictionGenerator" class="ee.webmedia.alfresco.docconfig.generator.systematic.AccessRestrictionGenerator">
      <property name="nodeService" ref="NodeService" />
      <property name="adrService" ref="AdrService" />
      <property name="documentLogService" ref="DocumentLogService" />
      <property name="documentAdminService" ref="DocumentAdminService" />
      <property name="documentConfigService" ref="DocumentConfigService" />
   </bean>

   <bean id="userContactTableGenerator" class="ee.webmedia.alfresco.docconfig.generator.systematic.UserContactTableGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
      <property name="namespaceService" ref="NamespaceService" />
      <property name="userContactMappingService" ref="UserContactMappingService" />
   </bean>

   <bean id="userContactRelatedGroupGenerator" class="ee.webmedia.alfresco.docconfig.generator.systematic.UserContactRelatedGroupGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
      <property name="namespaceService" ref="NamespaceService" />
      <property name="userContactMappingService" ref="UserContactMappingService" />
      <property name="documentAdminService" ref="DocumentAdminService" />
   </bean>

   <bean id="templateNameGenerator" class="ee.webmedia.alfresco.docconfig.generator.systematic.TemplateNameGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
   </bean>

   <bean id="regNumberGenerator" class="ee.webmedia.alfresco.docconfig.generator.systematic.RegNumberGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
      <property name="userService" ref="UserService" />
      <property name="regNumberEditable" value="${conf.document-regnumber.editable}" />
   </bean>

   <bean id="durationGenerator" class="ee.webmedia.alfresco.docconfig.generator.systematic.DurationGenerator">
      <property name="documentConfigService" ref="DocumentConfigService" />
      <property name="namespaceService" ref="NamespaceService" />
   </bean>

   <!-- START: web beans: -->

   <bean id="PropertySheetStateBean" class="ee.webmedia.alfresco.docconfig.web.PropertySheetStateBean" scope="session" />

   <!-- END: web beans: -->

</beans>
