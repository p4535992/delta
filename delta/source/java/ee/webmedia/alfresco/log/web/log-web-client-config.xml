<?xml version="1.0" encoding="UTF-8"?>
<alfresco-config>
   <config>
      <dialogs>
         <dialog name="applicationLog" page="/WEB-INF/classes/ee/webmedia/alfresco/log/web/application-log-dialog.jsp" managed-bean="ApplicationLogDialog"
            icon="/images/icons/versioned_properties_large.gif" title-id="applog" show-ok-button="false" />
         <dialog name="applicationLogList" page="/WEB-INF/classes/ee/webmedia/alfresco/log/web/application-log-list-dialog.jsp" managed-bean="ApplicationLogListDialog"
            icon="/images/icons/versioned_properties_large.gif" title-id="applog" show-ok-button="false" />
      </dialogs>
   </config>

   <config evaluator="node-type" condition="log:logFilter">
      <property-sheet>
         <show-property name="log:logEntryId" display-label-id="applog_item_id" />
         <show-property name="log:dateCreatedStart" display-label-id="applog_item_created" component-generator="InlinePropertyGroupGenerator" textId="applog_item_created_range"
            props="log:dateCreatedStart,log:dateCreatedEnd,log:dateCreatedStart_DateRangePicker|EnumSelectorGenerator|disableSorting=true|styleClass=quickDateRangePicker|enumClass=ee.webmedia.alfresco.classificator.enums.DatePeriods" />
         <show-property name="log:creatorName" display-label-id="applog_item_user" component-generator="SearchGenerator"
            pickerCallback="#{UserListDialog.searchUsers}" setterCallback="#{ApplicationLogDialog.processCreatorSearchResults}" dialogTitleId="users_search_title"/>
         <show-property name="log:computerId" display-label-id="applog_item_computer" />
         <show-property name="log:description" display-label-id="applog_item_description" />
         <show-property name="log:objectName" display-label-id="applog_item_object" />
         <show-property name="log:objectId" display-label-id="applog_item_object_id" />
      </property-sheet>
   </config>

</alfresco-config>
