   <bean id="abstractDocChildCreator" abstract="true" parent="propertiesModifierCallback">
      <property name="nodeService" ref="NodeService" />
      <property name="inMemoryChildNodeHelper" ref="inMemoryChildNodeHelper" />
   </bean>

   <bean class="ee.webmedia.alfresco.document.propmodifiers.ErrandApplicantAbroadPropertiesModifierCallback" parent="abstractDocChildCreator">
      <property name="parametersService" ref="ParametersService" />
   </bean>
   
