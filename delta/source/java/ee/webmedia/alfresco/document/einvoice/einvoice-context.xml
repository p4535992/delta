<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
   
   <bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
      <property name="ignoreUnresolvablePlaceholders" value="true" />
      <property name="location" value="classpath:ee/webmedia/alfresco/document/einvoice/bootstrap/dimensions-bootstrap.properties" />
   </bean>  
   
   <bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
      <property name="ignoreUnresolvablePlaceholders" value="true" />
      <property name="location" value="classpath:ee/webmedia/alfresco/document/einvoice/bootstrap/transactionDescParameters-bootstrap.properties" />
   </bean>  
   
   <bean id="eInvoiceService" class="ee.webmedia.alfresco.document.einvoice.service.EInvoiceServiceImpl">
      <property name="addressbookService" ref="AddressbookService" />
      <property name="userService" ref="UserService" />
      <property name="nodeService" ref="NodeService" />
      <property name="documentSearchService" ref="DocumentSearchService" />
      <property name="fileFolderService" ref="FileFolderService" />
      <property name="generalService" ref="GeneralService" />
      <property name="parametersService" ref="ParametersService" />
      <property name="documentTypeService" ref="DocumentTypeService" />
      <property name="documentTemplateService" ref="DocumentTemplateService" />
      <property name="fileService" ref="FileService" />
      <property name="dvkService" ref="DvkService" />
      <property name="documentLogService" ref="DocumentLogService" />
      <property name="dimensionsPath" value="${dimensions.parent.folder.xpath}${dimensions.folder.name}" />
      <property name="transactionDescParametersPath" value="${transactionDescParameters.parent.folder.xpath}${transactionDescParameters.folder.name}" />
   </bean>
   
   <bean id="EInvoiceService" class="org.springframework.aop.framework.ProxyFactoryBean">
      <property name="proxyInterfaces">
         <list>
            <value>ee.webmedia.alfresco.document.einvoice.service.EInvoiceService</value>
         </list>
      </property>
      <property name="target" ref="eInvoiceService" />
      <property name="interceptorNames">
         <list>
            <idref bean="commonTransactionInterceptor" />
         </list>
      </property>
   </bean>   
   
   <bean class="ee.webmedia.alfresco.document.einvoice.web.ReceivedInvoiceMenuItemProcessor">
      <property name="menuService" ref="MenuService" />
      <property name="documentService" ref="DocumentService" />
   </bean>
   
   <bean id="dimensionRootBootstrap" class="org.alfresco.repo.module.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="name" value="dimensionRootBootstrap" />
      <property name="executeOnceOnly" value="true" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="encoding">UTF-8</prop>
               <prop key="path">${dimensions.parent.folder.xpath}</prop>
               <prop key="location">ee/webmedia/alfresco/document/einvoice/bootstrap/dimensions.xml</prop>
               <prop key="messages">ee/webmedia/alfresco/document/einvoice/bootstrap/dimensions-bootstrap</prop>
            </props>
         </list>
      </property>
   </bean>
   
   <bean id="transactionDescParameterRootBootstrap" class="org.alfresco.repo.module.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="name" value="transactionDescParameterRootBootstrap" />
      <property name="executeOnceOnly" value="true" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="encoding">UTF-8</prop>
               <prop key="path">${transactionDescParameters.parent.folder.xpath}</prop>
               <prop key="location">ee/webmedia/alfresco/document/einvoice/bootstrap/transactionDescParameters.xml</prop>
               <prop key="messages">ee/webmedia/alfresco/document/einvoice/bootstrap/transactionDescParameters-bootstrap</prop>
            </props>
         </list>
      </property>
   </bean>   
   
   <bean id="transactionModelBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
      <property name="models">
         <list>
            <value>ee/webmedia/alfresco/document/einvoice/model/transactionModel.xml</value>
         </list>
      </property>
      <property name="labels">
         <list>
            <value>ee/webmedia/alfresco/document/einvoice/model/transaction-model</value>
         </list>
      </property>
   </bean>
   
   <bean id="einvoiceParametersBootstrap" class="org.alfresco.repo.module.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="name" value="einvoiceParametersBootstrap" />
      <property name="executeOnceOnly" value="true" />
      <property name="dependsOn">
         <list>
            <ref bean="parametersSpacesBootstrap" />
         </list>
      </property>
      <property name="importer" ref="spacesBootstrap" />
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="path">/param:parameters</prop>
               <prop key="location">ee/webmedia/alfresco/document/einvoice/bootstrap/einvoice-parameters.xml</prop>
               <prop key="encoding">UTF-8</prop>
            </props>
         </list>
      </property>
   </bean>
   
   <bean id="einvoiceParametersBootstrap1" class="org.alfresco.repo.module.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="name" value="einvoiceParametersBootstrap1" />
      <property name="executeOnceOnly" value="true" />
      <property name="dependsOn">
         <list>
            <ref bean="parametersSpacesBootstrap" />
         </list>
      </property>
      <property name="importer" ref="spacesBootstrap" />
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="path">/param:parameters</prop>
               <prop key="location">ee/webmedia/alfresco/document/einvoice/bootstrap/einvoice-parameters1.xml</prop>
               <prop key="encoding">UTF-8</prop>
            </props>
         </list>
      </property>
   </bean>  
   
   <bean id="dimensionModelBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
      <property name="models">
         <list>
            <value>ee/webmedia/alfresco/document/einvoice/model/dimensionModel.xml</value>
         </list>
      </property>
   </bean>
   
   <bean id="transactionDescParameterModelBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
      <property name="models">
         <list>
            <value>ee/webmedia/alfresco/document/einvoice/model/transactionDescParameterModel.xml</value>
         </list>
      </property>
   </bean>   

   <bean id="einvoiceWebClientConfigBootstrap" class="org.alfresco.web.config.WebClientConfigBootstrap" init-method="init"  depends-on="commonWebClientConfigBootstrap">
      <property name="configs">
         <list>
            <value>classpath:ee/webmedia/alfresco/document/einvoice/web/einvoice-web-client-config.xml</value>
         </list>
      </property>
   </bean>
   
   <bean id="einvoiceResourceBundleBootstrap" class="org.alfresco.web.app.ResourceBundleBootstrap">
      <property name="resourceBundles">
         <list>
            <value>ee.webmedia.alfresco.document.einvoice.web.einvoice-webclient</value>
         </list>
      </property>
   </bean>
   
   <bean class="ee.webmedia.alfresco.common.bootstrap.ModuleComponentDependsOnSetter">
      <property name="source" ref="parameterRescheduledJobsBootstrap" />
      <property name="dependsOn" ref="einvoiceParametersBootstrap" />
   </bean>   
   
   <bean id="einvoiceUpdateDocumentSapAccountTrigger" class="ee.webmedia.alfresco.parameters.job.ParameterRescheduledTriggerBean" parent="parameterRescheduledTriggerParent">
      <property name="parameterName" value="sapContactUpdateTime" />
      <property name="parameterFormat" value="H:mm" />
      <property name="jobDetail">
         <bean id="dvkReceiveOrganizationsJobDetails" class="org.springframework.scheduling.quartz.JobDetailBean">
            <property name="jobClass" value="ee.webmedia.alfresco.document.einvoice.job.EinvoiceUpdateDocumentSapAccountJob" />
            <property name="jobDataAsMap">
               <map>
                  <entry key="EInvoiceService" value-ref="EInvoiceService" />
               </map>
            </property>
         </bean>
      </property>
   </bean>
   
</beans>