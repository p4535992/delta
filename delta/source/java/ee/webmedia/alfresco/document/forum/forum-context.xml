<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

   <bean class="org.alfresco.web.app.ResourceBundleBootstrap">
      <property name="resourceBundles">
         <list>
            <value>ee/webmedia/alfresco/document/forum/web/forum-webclient</value>
         </list>
      </property>
   </bean>

   <bean class="org.alfresco.web.config.WebClientConfigBootstrap" init-method="init" depends-on="commonWebClientConfigBootstrap">
      <property name="configs">
         <list>
            <value>classpath:ee/webmedia/alfresco/document/forum/web/forum-web-client-config.xml</value>
         </list>
      </property>
   </bean>

   <bean id="forumPermissionRefactorUpdater" class="ee.webmedia.alfresco.document.forum.bootstrap.ForumPermissionRefactorUpdater" parent="abstractNodeUpdater" >
      <property name="moduleId" value="simdhs" />
      <property name="executeOnceOnly" value="true" />
   </bean>

   <bean class="ee.webmedia.alfresco.document.forum.web.DiscussionsMenuItemProcessor">
      <property name="menuService" ref="MenuService" />
      <property name="documentSearchService" ref="DocumentSearchService" />
   </bean>
</beans>