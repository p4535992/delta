<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

   <bean parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
      <property name="models">
         <list>
            <value>ee/webmedia/alfresco/document/type/model/documentTypeModel.xml</value>
         </list>
      </property>
   </bean>

   <bean class="org.alfresco.web.config.WebClientConfigBootstrap" init-method="init" depends-on="commonWebClientConfigBootstrap">
      <property name="configs">
         <list>
            <value>classpath:ee/webmedia/alfresco/document/type/web/document-type-web-client-config.xml</value>
         </list>
      </property>
   </bean>

   <bean class="org.alfresco.web.app.ResourceBundleBootstrap">
      <property name="resourceBundles">
         <list>
            <value>ee/webmedia/alfresco/document/type/web/document-type-webclient</value>
         </list>
      </property>
   </bean>

   <bean id="documentTypeSpacesBootstrap" class="org.alfresco.repo.module.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="name" value="documentTypeSpacesBootstrap" />
      <property name="sinceVersion" value="1.1.1.20" />
      <property name="executeOnceOnly" value="true" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="encoding">UTF-8</prop>
               <prop key="path">/</prop>
               <prop key="location">ee/webmedia/alfresco/document/type/bootstrap/documentTypes.xml</prop>
            </props>
         </list>
      </property>
   </bean>
   
   <bean id="documentTypeSpacesBootstrap2" class="org.alfresco.repo.module.ImporterModuleComponent" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="name" value="documentTypeSpacesBootstrap2" />
      <property name="sinceVersion" value="2.1.1" />
      <property name="executeOnceOnly" value="true" />
      <property name="importer" ref="spacesBootstrap" />
      <property name="bootstrapViews">
         <list>
            <props>
               <prop key="encoding">UTF-8</prop>
               <prop key="path">/doctype:documentTypes</prop>
               <prop key="location">ee/webmedia/alfresco/document/type/bootstrap/documentTypes2.xml</prop>
            </props>
         </list>
      </property>
   </bean>   

   <bean id="documentTypeService" class="ee.webmedia.alfresco.document.type.service.DocumentTypeServiceImpl">
      <property name="generalService" ref="GeneralService" />
      <property name="namespaceService" ref="NamespaceService" />
      <property name="nodeService" ref="NodeService" />
      <property name="menuService" ref="MenuService" />
   </bean>

   <bean id="DocumentTypeService" class="org.springframework.aop.framework.ProxyFactoryBean">
      <property name="proxyInterfaces">
         <list>
            <value>ee.webmedia.alfresco.document.type.service.DocumentTypeService</value>
         </list>
      </property>
      <property name="target" ref="documentTypeService" />
      <property name="interceptorNames">
         <list>
            <idref bean="commonTransactionInterceptor" />
         </list>
      </property>
   </bean>

   <bean class="ee.webmedia.alfresco.document.type.service.DocumentTypeMenuItemProcessor">
      <property name="menuService" ref="MenuService" />
      <property name="documentTypeService" ref="DocumentTypeService" />
      <property name="namespaceService" ref="NamespaceService" />
   </bean>

   <bean id="changeVacationOrderNameBootstrap" class="ee.webmedia.alfresco.document.type.bootstrap.ChangeVacationOrderNameBootstrap" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="name" value="changeVacationOrderNameBootstrap" />
      <property name="sinceVersion" value="1.9.1.13" />
      <property name="executeOnceOnly" value="true" />
      <property name="generalService" ref="GeneralService" />
      <property name="dependsOn">
         <list>
            <ref bean="documentTypeSpacesBootstrap" />
         </list>
      </property>
   </bean>
   
   <bean id="changeMinistersOrderNameBootstrap" class="ee.webmedia.alfresco.document.type.bootstrap.ChangeMinistersOrderNameBootstrap" parent="module.baseComponent">
      <property name="moduleId" value="simdhs" />
      <property name="name" value="changeMinistersOrderNameBootstrap" />
      <property name="sinceVersion" value="2.2.1.1" />
      <property name="executeOnceOnly" value="true" />
      <property name="generalService" ref="GeneralService" />
      <property name="dependsOn">
         <list>
            <ref bean="documentTypeSpacesBootstrap" />
         </list>
      </property>
   </bean>     

</beans>
