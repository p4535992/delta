            <aspect>docspec:errandApplicant</aspect>
            <aspect>docspec:errandApplicationApplicantDomestic</aspect>
         </mandatory-aspects>
      </type>

      <type name="docspec:trainingApplicationApplicantType"><!-- Koolitustaotluse taotleja(dokument->taotleja) -->
