            </property>
            <property name="docsearch:signerName">
               <type>d:text</type>
            </property>
            <property name="docsearch:signerJobTitle">
               <type>d:text</type>
            </property>
            <property name="docsearch:keywords">
               <type>d:text</type>
            </property>
            <property name="docsearch:storageType">
               <type>d:text</type>
               <multiple>true</multiple>
            </property>
   
