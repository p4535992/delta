<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

   <bean id="dvkReceiveOrganizationsTrigger" class="ee.webmedia.alfresco.parameters.job.ParameterResceduledTriggerBean" parent="parameterResceduledTriggerParent">
      <property name="parameterName" value="dvkReceiveOrganizations" />
      <property name="parameterFormat" value="H:mm" />
      <property name="jobDetail">
         <bean id="dvkReceiveOrganizationsJobDetails" class="org.springframework.scheduling.quartz.JobDetailBean">
            <property name="jobClass" value="ee.webmedia.alfresco.addressbook.job.DvkReceiveOrganizationsJob" />
            <property name="jobDataAsMap">
               <map>
                  <entry key="AddressbookService" value-ref="AddressbookService" />
               </map>
            </property>
         </bean>
      </property>
   </bean>

</beans>
