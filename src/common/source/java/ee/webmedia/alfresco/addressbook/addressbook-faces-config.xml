<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

   <!-- ==================== MANAGED BEANS ==================== -->

   <managed-bean>
      <managed-bean-name>AddressbookHtmlSeparatorGenerator</managed-bean-name>
      <managed-bean-class>org.alfresco.web.bean.generator.HtmlSeparatorGenerator</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
      <managed-property>
         <property-name>html</property-name>
         <value>
				<![CDATA[<div id="addressbook_browse_button">button</div>]]>
         </value>
      </managed-property>
   </managed-bean>

   <managed-bean>
      <description>The bean that holds state for the Addressbook Management screens.</description>
      <managed-bean-name>AddressbookDialog</managed-bean-name>
      <managed-bean-class>ee.webmedia.alfresco.addressbook.web.dialog.AddressbookMainViewDialog</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
      <managed-property>
         <property-name>addressbookService</property-name>
         <value>#{AddressbookService}</value>
      </managed-property>
   </managed-bean>

   <managed-bean>
      <description>The bean that holds state for the Addressbook Management screens.</description>
      <managed-bean-name>AddressbookDeleteDialog</managed-bean-name>
      <managed-bean-class>ee.webmedia.alfresco.addressbook.web.dialog.AddressbookDeleteDialog</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
      <managed-property>
         <property-name>addressbookService</property-name>
         <value>#{AddressbookService}</value>
      </managed-property>
   </managed-bean>
   
   <managed-bean>
      <managed-bean-name>AddressbookAddEditDialog</managed-bean-name>
      <managed-bean-class>ee.webmedia.alfresco.addressbook.web.dialog.AddressbookAddEditDialog</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
      <managed-property>
         <property-name>addressbookService</property-name>
         <value>#{AddressbookService}</value>
      </managed-property>
   </managed-bean>

   <managed-bean>
      <managed-bean-name>ContactGroupListDialog</managed-bean-name>
      <managed-bean-class>ee.webmedia.alfresco.addressbook.web.dialog.ContactGroupListDialog</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
      <managed-property>
         <property-name>addressbookService</property-name>
         <value>#{AddressbookService}</value>
      </managed-property>
   </managed-bean>
   
   <managed-bean>
      <managed-bean-name>ContactGroupContactsDialog</managed-bean-name>
      <managed-bean-class>ee.webmedia.alfresco.addressbook.web.dialog.ContactGroupContactsDialog</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
      <managed-property>
         <property-name>addressbookService</property-name>
         <value>#{AddressbookService}</value>
      </managed-property>
   </managed-bean>
   
   <managed-bean>
      <managed-bean-name>ContactGroupCreateDialog</managed-bean-name>
      <managed-bean-class>ee.webmedia.alfresco.addressbook.web.dialog.ContactGroupCreateDialog</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
      <managed-property>
         <property-name>addressbookService</property-name>
         <value>#{AddressbookService}</value>
      </managed-property>
   </managed-bean>
   
   <managed-bean>
      <managed-bean-name>ContactGroupAddDialog</managed-bean-name>
      <managed-bean-class>ee.webmedia.alfresco.addressbook.web.dialog.ContactGroupAddDialog</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
      <managed-property>
         <property-name>addressbookService</property-name>
         <value>#{AddressbookService}</value>
      </managed-property>
   </managed-bean>
   
   <managed-bean>
      <managed-bean-name>ContactGroupDeleteDialog</managed-bean-name>
      <managed-bean-class>ee.webmedia.alfresco.addressbook.web.dialog.ContactGroupDeleteDialog</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
      <managed-property>
         <property-name>addressbookService</property-name>
         <value>#{AddressbookService}</value>
      </managed-property>
   </managed-bean>
   
</faces-config>
