<?xml version="1.0" encoding="UTF-8"?>
<model name="param:paramModel" xmlns="http://www.alfresco.org/model/dictionary/1.0" schemaLocation="http://www.alfresco.org/model/dictionary/1.0 http://svn.alfresco.com/repos/alfresco-open-mirror/alfresco/HEAD/root/projects/repository/config/alfresco/model/modelSchema.xsd">

   <imports>
      <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
      <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys" />
      <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
   </imports>

   <namespaces>
      <namespace uri="http://alfresco.webmedia.ee/model/parameters/1.0" prefix="param" />
   </namespaces>

   <types>
      <type name="param:parameters">
         <parent>sys:base</parent>
         <associations>
            <child-association name="param:parameter">
               <source>
                  <mandatory>true</mandatory>
                  <many>false</many>
               </source>
               <target>
                  <class>param:parameter</class>
                  <mandatory>false</mandatory>
                  <many>true</many>
               </target>
            </child-association>
         </associations>
         <mandatory-aspects>
            <aspect>sys:aspect_root</aspect>
         </mandatory-aspects>
      </type>

      <type name="param:parameter">
         <parent>sys:base</parent>
         <properties>
            <property name="param:value">
               <type>d:any</type>
               <mandatory>true</mandatory>
            </property>
         </properties>
      </type>

      <type name="param:stringParameter">
         <parent>param:parameter</parent>
      </type>

      <type name="param:intParameter">
         <parent>param:parameter</parent>
      </type>

      <type name="param:doubleParameter">
         <parent>param:parameter</parent>
      </type>
   </types>
   
</model>
