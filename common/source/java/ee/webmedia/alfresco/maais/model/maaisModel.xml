<?xml version="1.0" encoding="UTF-8"?>
<model name="maais:maaisModel" xmlns="http://www.alfresco.org/model/dictionary/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.alfresco.org/model/dictionary/1.0 http://svn.alfresco.com/repos/alfresco-open-mirror/alfresco/HEAD/root/projects/repository/config/alfresco/model/modelSchema.xsd">

   <imports>
      <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
      <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys" />
      <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
      <import uri="http://alfresco.webmedia.ee/model/document/common/1.0" prefix="doccom" />
   </imports>

   <namespaces>
      <namespace uri="http://alfresco.webmedia.ee/model/maais/1.0" prefix="maais" />
   </namespaces>

   <types>
      <type name="maais:maaisCase">
         <parent>sys:base</parent>
         <mandatory-aspects>
            <aspect>maais:maaisCaseAspect</aspect>
         </mandatory-aspects>
      </type>
   </types>

   <aspects>

      <aspect name="maais:maaisCaseAspect">
         <properties>
            <property name="maais:caseNumber">
               <type>d:text</type>
               <mandatory>true</mandatory>
            </property>
            <property name="maais:caseRelatedPerson">
               <type>d:text</type>
               <mandatory>true</mandatory>
            </property>
            <property name="maais:landNumber">
               <type>d:text</type>
               <mandatory>true</mandatory>
            </property>
            <property name="maais:landName">
               <type>d:text</type>
               <mandatory>true</mandatory>
            </property>
         </properties>
         <associations>
            <association name="maais:maaisCaseDocument">
               <source>
                  <mandatory>false</mandatory>
                  <many>true</many>
               </source>
               <target>
                  <class>doccom:document</class>
                  <mandatory>false</mandatory>
                  <many>true</many>
               </target>
            </association>
         </associations>
      </aspect>

      <aspect name="maais:maaisNotifyAssoc" />

   </aspects>

</model>
