<?xml version="1.0" encoding="UTF-8"?>
<model name="ocr:ocrModel" xmlns="http://www.alfresco.org/model/dictionary/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.alfresco.org/model/dictionary/1.0 http://svn.alfresco.com/repos/alfresco-open-mirror/alfresco/HEAD/root/projects/repository/config/alfresco/model/modelSchema.xsd">

   <imports>
      <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
      <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys" />
   </imports>

   <namespaces>
      <namespace uri="http://alfresco.webmedia.ee/model/ocr/1.0" prefix="ocr" />
   </namespaces>

   <aspects>

      <aspect name="ocr:ocrCompleted">
         <properties>
            <property name="ocr:ocrLog">
               <type>d:text</type>
            </property>
            <property name="ocr:ocrStartedDateTime">
               <type>d:datetime</type>
            </property>
            <property name="ocr:ocrCompletedDateTime">
               <type>d:datetime</type>
            </property>
         </properties>
      </aspect>   

   </aspects>

</model>
