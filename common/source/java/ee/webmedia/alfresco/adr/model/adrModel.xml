            <aspect>sys:aspect_root</aspect>
         </mandatory-aspects>
      </type>

      <type name="adr:adrDeletedDocumentType">
         <parent>sys:base</parent>
         <mandatory-aspects>
            <aspect>adr:documentType</aspect>
            <aspect>adr:deletedDateTime</aspect>
         </mandatory-aspects>
      </type>

      <type name="adr:adrAddedDocumentTypes">
