								<xs:annotation>
									<xs:documentation>Arhivaali osaks olev fail koos tehniliste metaandmetega</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="KirjeldusyksusType">
		<xs:annotation>
			<xs:documentation>Kirjeldusüksuse andmete tüüp</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="KyTasand" type="KyTasandImportEnumerationType">
				<xs:annotation>
					<xs:documentation>Kirjeldusüksuse tasand (arhiiv, allarhiiv, sari, allsari, säilik, arhivaal) </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KyIdentiteediala" type="KyIdentiteedialaImportType">
				<xs:annotation>
					<xs:documentation>Kirjeldusüksust identifitseerivad metaandmed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KySisuStruktAla" type="KySisuStruktAlaType">
				<xs:annotation>
					<xs:documentation>Kirjeldusüksuse sisu ja struktuuri metaandmed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KyJuurdepaasuala" type="JuurdepaasualaImportType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Kirjeldusüksuse kasutamise piirangut kirjeldavad metaandmed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KySeonduvAines" type="SeotudOsaImportType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Päritolu kaudu või muul moel seotud kirjeldusüksus  kas samas või mujal arhiivis. Selgitatakse kirjeldusüksuste vaheliste seoste iseloomu.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KyPublikatsioon" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Määratletakse publikatsioonid, mille aluseks on olnud kirjeldusüksuse kasutamine, uurimine või analüüs.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KyKirjeldusAndmed" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Kirjelduse koostamise või muutmise aeg ning koostaja(te) või muutja(te) nimi või nimed.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Nimi" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Kirjelduse koostaja nimi</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Aeg" type="LaiendatudKuupaevType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Kirjeldusüksuse kirjelduse loomise kuupäev</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Tegevus" type="KoostamineTegevusEnumerationType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Kirjeldusega tehtud tegevus (loomine, täiendamine)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="KyKasutatudAllikad" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Määratletakse allikmaterjalid, mis on olnud aluseks käesoleva kirjelduse koostamisel.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KyMarkused" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Kasutatakse vaid juhul, kui olulist informatsiooni ei ole võimalik kanda teistesse kirjelduselementidesse</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KyTegevus" type="TegevusImportType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Tegevused kirjeldusüksusega</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KyIdentiteedialaImportType">
		<xs:annotation>
			<xs:documentation>Kirjeldusüksuse identifitseerivate metaandmete tüüp</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="KyIdent" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Kirjeldusüksuse identifikaator (normatiivne viitekood pikal kujul), arhiivi viitekood</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KyViit" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tähis algsüsteemis (Alternatiivne viitekood). Mittevajalik arhiivi ja allarhiivi kirjelduses</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KyIdEDHS" type="xs:string" minOccurs="0">
				<xs:annotation>
