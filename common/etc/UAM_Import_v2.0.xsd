					<xs:documentation>Fond-Nimistu-Säilik numeratsioon</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EelmineFNS" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Varasem Fond-Nimistu-Säilik numeratsioon</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KyPealkiri" type="xs:string">
