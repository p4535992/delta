<xs:schema xmlns="http://www.ra.ee/schemas/EDHS/import_v0.1" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.ra.ee/schemas/EDHS/import_v0.1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.1 18.11.2009">
	<xs:element name="UAM_import">
		<xs:annotation>
			<xs:documentation>UAM-i laaditav aines - arhiivikirjeldus ja dokumendid</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Arhiivikirjeldus" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Kirjeldusüksuste ja funktsioonide metaandmed</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Kirjeldusyksus" type="KirjeldusyksusType" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Liigitusüksuste (sari, säilik) kirjeldused</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Funktsioon" type="FunktsioonImportType" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Funktsioonide ja volituste kirjeldused</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Arhivaal" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>UAM-i laaditava arhivaali kapsli struktuur</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="KyMeta" type="KyMetaType">
